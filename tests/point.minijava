interface ObjetGeometrique {
	int getX();
	int getY();
	void setX(int mx);
	void setY(int my);
}

class Point implements ObjetGeometrique {
	public int x;
	public int y;
	
	public Point(int mx, int my) {
		this.x = mx;
		this.y = my;
	}
	public Point() {}
	
	public int getX(){ 
		return this.x; 
	}
	public int getY(){ 
		return this.y; 
	}
	
	public void setX(int mx){ this.x = mx; }
	public void setY(int my){ this.y = my; }	

}


class PointNomme extends Point {
	private String name;
	
	public String getName(){
		return this.name;
	}
	
	public PointNomme(String nom){
		this.name = nom;
	}
}